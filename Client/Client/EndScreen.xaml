<Window x:Class="Client.EndScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client"
        mc:Ignorable="d"
        Title="EndScreen" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="https://www.videomeme.in/wp-content/uploads/2023/04/speed-skipping-screen.jpg"/>
        </Grid.Background>
        <ListView x:Name="resultsView" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="320,47,75,148" SelectionChanged="ListView_SelectionChanged">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>
        <Button x:Name="leave" Content="return to lobby" HorizontalAlignment="Left" Margin="65,84,0,0" VerticalAlignment="Top" Width="107" Click="leave_Click"/>
    </Grid>
</Window>
