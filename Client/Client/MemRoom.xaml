<Window x:Class="Client.MemRoom"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client"
        mc:Ignorable="d"
        Title="MemRoom" Height="450" Width="800">
    <Grid Margin="0,10,0,0">
        <Grid.Background>
            <ImageBrush ImageSource="https://www.pcgamesn.com/wp-content/sites/pcgamesn/2021/04/rust-poker-april-update.jpg"/>
        </Grid.Background>
        <ListView x:Name="membersList" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="310,76,310,158" SelectionChanged="membersList_SelectionChanged">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>
        <Button x:Name="leave" Content="Leave Room" Margin="32,360,638,26" Click="leave_Click"/>
    </Grid>

</Window>
