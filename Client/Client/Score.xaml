<Window x:Class="Client.Score"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client"
        mc:Ignorable="d"
        Title="Score" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="https://cdn.mos.cms.futurecdn.net/X3LBHs4Zfx7SbyvJ86BnmW.jpg"/>
        </Grid.Background>
        <ListBox x:Name="scores" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="623,10,10,164"/>
        <Button x:Name="leave" Content="Leave" HorizontalAlignment="Left" Margin="27,32,0,0" VerticalAlignment="Top" Height="25" Width="54" Click="leave_Click"/>


    </Grid>
</Window>
